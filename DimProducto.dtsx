                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
DP.ProductKey AS IDProducto,
DP.ProductName AS NombreProducto,
DPS.ProductSubcategoryName AS Subcategoria,
DPC.ProductCategoryName AS Categoria,
DP.BrandName AS Marca,
CASE 
	WHEN DP.ColorName = 'blue' THEN 'Azul'
	WHEN DP.ColorName = 'Red' THEN 'Rojo'
	ELSE DP.ColorName END Color,
DP.UnitCost as CostoUnitario,
DP.UnitPrice PrecioUnitario
FROM Stage.dbo.DimProduct DP
LEFT JOIN Stage.dbo.DimProductSubcategory DPS
ON DP.ProductSubcategoryKey = DPS.ProductSubcategoryKey
LEFT JOIN Stage.dbo.DimProductCategory DPC
ON DPS.ProductCategoryKey = DPC.ProductCategoryKey</property>
